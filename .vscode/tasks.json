{
	"version": "2.0.0",
	"tasks": [
		{
			"label": "C++ Build",
			"type": "shell",
			"command": "C:/MinGW/bin/g++.exe",
			"args": [
				"-Wall",
				"-Wextra",
				"-g3",
				"${file}",
				"-o",
				"${fileDirname}/${fileBasenameNoExtension}.exe"
			],
			"group": {
				"kind": "build",
				"isDefault": true
			},
			"problemMatcher": [
				"$gcc"
			],
			"detail": "Generated by VS Code"
		}
	]
}